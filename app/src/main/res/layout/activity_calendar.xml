<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ragda.pushup.calendar">


    <ImageView
        android:layout_width="30dp"
        android:layout_height="28dp"
        android:id="@+id/imageView5"
        android:src="@drawable/s2"
        android:layout_alignTop="@+id/imageView19"
        android:layout_toStartOf="@+id/imageView4"
        android:layout_alignBottom="@+id/imageView19"
        android:layout_toLeftOf="@+id/imageView4" />

    <ImageView
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:id="@+id/imageView4"
        android:src="@drawable/s1"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="0dp"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_alignRight="@+id/calendar"
        android:layout_alignEnd="@+id/calendar" />

    <CalendarView
        android:layout_width="match_parent"
        android:layout_height="455dp"
        android:id="@+id/calendar"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="43dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView19"
        android:layout_toStartOf="@+id/imageView5"
        android:src="@drawable/s3"
        android:layout_alignBottom="@+id/imageView20"
        android:layout_toLeftOf="@+id/imageView5" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView20"
        android:src="@drawable/s4"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/imageView19"
        android:layout_toStartOf="@+id/imageView19" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView21"
        android:src="@drawable/s5"
        android:layout_alignBottom="@+id/imageView20"
        android:layout_toLeftOf="@+id/imageView20"
        android:layout_toStartOf="@+id/imageView20" />




</RelativeLayout>
